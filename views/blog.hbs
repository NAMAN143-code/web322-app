  {{#each data.posts}}  
<div class="row">  
    
    <section class="main col-md-9">
        <article>
            <h2>{{title}}</h2>
            <img src={{featureImage}} alt="feature Image" style="width:100%" />
            <br /><br />
         {{body}}
            <br /><br />
            <strong>Category: </strong> {{category}}<br />
            <strong>Last Updated: </strong> {{postDate}}

        </article>
    </section>

    <aside class="col-md-3">
        <br /><br />
    {{#if data.posts}}
        <h4>Posts</h4>

        <ul class="list-group">
        <a href="/blog/{{id}}?category={{../viewingCategory}}" class="list-group-item">{{title}}</a>
        </ul>
    {{/if}}
        <h4>Categories</h4>

        <ul class="list-group">
           <a href="/blog?category={{category}}" class="list-group-item">{{category}}</a>

        </ul>
    </aside>   
</div>

    {{/each}}